/* ToolTips */
#tooltip
{
	background-color: #EEEEEE;
	border: 1px solid #111111;
	opacity: 0.85;
	padding: 5px;
	position: absolute;
	z-index: 3000;
}
#tooltip h3, #tooltip div
{
	margin: 0;
}

#tooltip.pretty
{
	background: url("/images/tooltip.png") repeat scroll 0 0 transparent;
	border: medium none;
	height: 310px;
	opacity: 0.9;
	padding: 20px;
	width: 460px;
	text-align: center;
}

#tooltip.pretty h3
{
	font-size: 16px;
	/*margin-bottom: 0.75em;*/
